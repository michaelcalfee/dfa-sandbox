{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "commonjs": true,
        "jquery": true,
        "es6": true
    },
    "globals": {

    },
    "rules": {
        "no-console": 0,
        "react/jsx-uses-vars": 1,
        "react/jsx-uses-react": 1
    },
    "plugins": [
        "react"
    ]
}